@model IEnumerable<CreditWorks.Models.Vehicle_Category>

@{
    ViewBag.Title = "index";
}

<h2>Category</h2>

<p>
    @Html.ActionLink("Add Category", "create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CategoryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RangeMin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RangeMax)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CategoryIcon)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CategoryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RangeMin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RangeMax)
        </td>
        <td>
            <span class="@Html.DisplayFor(modelItem => item.CategoryIcon)"></span>          
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
